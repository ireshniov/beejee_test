{
    "require": {
        "php": ">=7.1",
        "symfony/http-foundation": "^4.3",
        "symfony/routing": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/event-dispatcher": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "doctrine/orm": "^2.6",
        "doctrine/migrations": "^2.1",
        "twig/twig": "^2.11",
        "symfony/dotenv": "^4.3",
        "symfony/form": "^4.3",
        "symfony/security-csrf": "^4.3",
        "symfony/twig-bridge": "^4.3",
        "symfony/translation": "^4.3",
        "symfony/config": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
